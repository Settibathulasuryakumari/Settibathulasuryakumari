#include <stdio.h>
int main() {
    char ch;
    scanf("%c",&ch);
    if(ch>='A'&&ch<='Z'){
        printf("%c is a uppercase alphabet",ch);
    }
    else if(ch>='a'&&ch<='z'){
        printf("%c is a lowercase alphabet",ch);
    }
    else if(ch>='0'||ch<'0'){
         printf("%c is an integer",ch);
    }
    else{
     printf("%c is a special character",ch);
}
    return 0;
}
